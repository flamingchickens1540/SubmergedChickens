model Comparison {
  id              Int       @id @default(autoincrement())
  team_match_A_id Int
  team_match_A    TeamMatch @relation("TeamMatchA", fields: [team_match_A_id], references: [id])
  team_match_B_id Int
  team_match_B    TeamMatch @relation("TeamMatchB", fields: [team_match_B_id], references: [id])
  diff            Int
  category        String
  event_key       String
  event           Event     @relation(fields: [event_key], references: [event_key])
  user_id         Int
  user            User      @relation(fields: [user_id], references: [id])
}
